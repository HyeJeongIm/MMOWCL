{
  "dataset": "mmea-tbn",
  "modality": ["RGB", "Gyro", "Acce"],
  "model_name": "tbn_lwf",
  
  "backbone": "tbn",
  "arch": "BNInception",
  "midfusion": "concat",
  "consensus_type": "avg",
  "before_softmax": true,
  
  "train_list": "mydataset_train.txt",
  "test_list": "mydataset_test.txt",
  "mpu_path": "./data/UESTC-MMEA-CL/mpu/",
  
  "num_segments": 8,
  "batch_size": 8,
  "workers": 4,
  "dropout": 0.5,
  
  "lr": 0.001,
  "lr_steps": [10, 20],
  "momentum": 0.9,
  "weight_decay": 0.0005,
  "epochs": 10, 
  "clip_gradient": 20,
  
  "memory_size": 0,
  "increment": 4,
  "shuffle": false,
  
  "partialbn": true,
  "freeze": false,
  
  "enable_ood": true,
  "ood_methods": ["MSP", "ODIN", "Energy"],
  
  "seed": 1993,
  "device": [0],
  "prefix": "mmea-owcl",
  "log_test_acc": false
}